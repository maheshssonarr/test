<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sonar.secrets.java"
    android:versionCode="73"
    android:versionName="7.3" >

     
    
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />
    
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.sonar.secrets.Login"
            android:label="@string/app_name"
            android:windowSoftInputMode="stateVisible|adjustPan" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sonar.secrets.Secrets"
            android:label="@string/title_activity_secrets"
            android:parentActivityName="com.sonar.secrets.Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sonar.secrets.Login" />
        </activity>
        <activity
            android:name="com.sonar.secrets.Help"
            android:label="@string/title_activity_help" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sonar.secrets.Login" />
        </activity>
        <activity
            android:name="com.sonar.secrets.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.sonar.secrets.Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sonar.secrets.Login" />
        </activity>
        <activity
            android:name="com.sonar.secrets.TabsActivity"
            android:label="@string/title_activity_android_tab_layout"
            android:parentActivityName="com.sonar.secrets.Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sonar.secrets.Login" />
        </activity>
        <activity
            android:name="com.sonar.secrets.PhotosActivity"
            android:label="@string/title_activity_photos"
            android:parentActivityName="com.sonar.secrets.AndroidTabLayoutActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sonar.secrets.AndroidTabLayoutActivity" />
        </activity>
        <activity
            android:name="com.sonar.secrets.SongsActivity"
            android:label="@string/title_activity_songs"
            android:parentActivityName="com.sonar.secrets.AndroidTabLayoutActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sonar.secrets.AndroidTabLayoutActivity" />
        </activity>
        <activity
            android:name="com.sonar.secrets.VideosActivity"
            android:label="@string/title_activity_videos"
            android:parentActivityName="com.sonar.secrets.AndroidTabLayoutActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sonar.secrets.AndroidTabLayoutActivity" />
        </activity>
        <activity
            android:name="com.sonar.secrets.FilesActivity"
            android:label="@string/title_activity_files"
            android:parentActivityName="com.sonar.secrets.AndroidTabLayoutActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sonar.secrets.AndroidTabLayoutActivity" />
        </activity>
        <activity
            android:name="com.sonar.secrets.Share"
            android:label="@string/title_activity_share" >
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <action android:name="android.intent.action.SEND_MULTIPLE" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="image/*" />
                <data android:mimeType="video/*" />
                <data android:mimeType="audio/*" />
                <data android:mimeType="*/*" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="image/*" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="text/plain" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND_MULTIPLE" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="image/*" />
            </intent-filter>
        </activity>
    </application>
	
	
</manifest>